{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "ext-bcmath": "*",
        "ext-dom": "*",
        "ext-fileinfo": "*",
        "ext-pdo": "*",
        "aws/aws-sdk-php": "^3.278",
        "commerceguys/addressing": "^2.0",
        "commerceguys/intl": "^2.0",
        "elasticsearch/elasticsearch": "^8.8",
        "google/apiclient": "^2.15",
        "hisorange/browser-detect": "^4.5",
        "inertiajs/inertia-laravel": "^0.6.8",
        "jackwh/laravel-new-relic": "^1.0",
        "kreait/laravel-firebase": "^5.2",
        "laravel/framework": "^10.10",
        "laravel/horizon": "^5.17",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8",
        "lcherone/php-petname": "^1.0",
        "league/flysystem-aws-s3-v3": "^3.0",
        "lorisleiva/laravel-actions": "^2.6",
        "maatwebsite/excel": "^3.1",
        "masbug/flysystem-google-drive-ext": "^2.2",
        "matchish/laravel-scout-elasticsearch": "^7.2",
        "owen-it/laravel-auditing": "^13.5",
        "propaganistas/laravel-phone": "^5.0",
        "pusher/pusher-php-server": "^7.2",
        "sentry/sentry-laravel": "^3.7",
        "spatie/crawler": "^8.0",
        "spatie/laravel-analytics": "^5.0",
        "spatie/laravel-medialibrary": "^10.0.0",
        "spatie/laravel-options": "^1.1",
        "spatie/laravel-permission": "^5.10",
        "spatie/laravel-query-builder": "^5.2",
        "spatie/laravel-sluggable": "^3.5",
        "spatie/laravel-tags": "^4.5",
        "stevebauman/location": "^7.0",
        "tightenco/ziggy": "^1.0",
        "vemcogroup/laravel-translation": "^3.0",
        "vlucas/phpdotenv": "^5.5",
        "xendit/xendit-php": "^2.19"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.13",
        "deployer/deployer": "^7.3",
        "fakerphp/faker": "^1.9.1",
        "laravel/breeze": "^1.21",
        "laravel/pint": "^1.10",
        "laravel/sail": "^1.18",
        "laravel/telescope": "^4.16",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "pestphp/pest": "^2.8",
        "pestphp/pest-plugin-faker": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.2",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
